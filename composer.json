{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "symfony/flex": "^1.4",
        "symfony/framework-bundle": "^4.3",
        "symfony/security-bundle": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "symfony/browser-kit": "^4.3",
        "symfony/phpunit-bridge": "^4.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Shwrm\\Tracking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Shwrm\\Tracking\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    }
}
